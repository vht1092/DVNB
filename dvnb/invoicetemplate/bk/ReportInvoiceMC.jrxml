	<subDataset name="Dataset1" uuid="9ce7341d-6568-4c62-b228-57d905a0885c">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="DW_LIVE"/>
		<parameter name="S_KY" class="java.lang.String"/>
		<parameter name="S_KETCHUYEN" class="java.lang.String"/>
		<parameter name="S_TUNGAY" class="java.lang.String"/>
		<parameter name="S_DENNGAY" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT SEQNO, DRIVER_CDE_ORDER, MA_DRIVER, TEN_DRIVER, GRP, MA_PHI, DIEN_GIAI_MA_PHI, SO_HOA_DON, NGAY_HOA_DON, SO_TIEN_USD,ROUND(SO_TIEN_USD*TYGIA+SO_TIEN_LECH) SO_TIEN_VND
		FROM 
		(    
		SELECT 0 SEQNO,DRIVER_CDE DRIVER_CDE_ORDER ,DRIVER_CDE MA_DRIVER,DRIVER_NAME TEN_DRIVER,' ' GRP,' ' MA_PHI,
		' ' DIEN_GIAI_MA_PHI,
		' ' SO_HOA_DON,' ' NGAY_HOA_DON,
		TO_NUMBER(SUM(TOTAL_CHARGE)) SO_TIEN_USD,' ' SO_TIEN_VND,SUM(DIAVITION) SO_TIEN_LECH,A.KY
		FROM DVNB_INVOICE_MC_TEMP A
		LEFT JOIN DVNB_BILLING_GRP B ON A.EVENT_ID=B.BILLING_CDE   
		WHERE ($P{S_KY}<>'All' AND KY = $P{S_KY} AND KET_CHUYEN = $P{S_KETCHUYEN})
		OR ($P{S_KY}='All' AND $P{S_KETCHUYEN}<>'All' AND (REPLACE(BILLING_CYCLE_DATE,'/','') BETWEEN $P{S_TUNGAY} AND $P{S_DENNGAY}) AND KET_CHUYEN=$P{S_KETCHUYEN})
		OR ($P{S_KY}='All' AND $P{S_KETCHUYEN}='All' AND (REPLACE(BILLING_CYCLE_DATE,'/','') BETWEEN $P{S_TUNGAY} AND $P{S_DENNGAY}))
		GROUP BY  DRIVER_CDE,DRIVER_NAME,KY
		UNION ALL
		SELECT 1 SEQNO,DRIVER_CDE DRIVER_CDE_ORDER,' ' MA_DRIVER,' ' TEN_DRIVER,GRP,EVENT_ID MA_PHI,
		EVENT_DESC DIEN_GIAI_MA_PHI,INVOICE_NUMBER SO_HOA_DON,BILLING_CYCLE_DATE NGAY_HOA_DON,
		TO_NUMBER(SUM(TOTAL_CHARGE)) SO_TIEN_USD,' ' SO_TIEN_VND,SUM(DIAVITION) SO_TIEN_LECH,A.KY
		FROM DVNB_INVOICE_MC_TEMP A
		LEFT JOIN DVNB_BILLING_GRP B ON A.EVENT_ID=B.BILLING_CDE
		WHERE ($P{S_KY}<>'All' AND KY = $P{S_KY} AND KET_CHUYEN = $P{S_KETCHUYEN})
		OR ($P{S_KY}='All' AND $P{S_KETCHUYEN}<>'All' AND (REPLACE(BILLING_CYCLE_DATE,'/','') BETWEEN $P{S_TUNGAY} AND $P{S_DENNGAY}) AND KET_CHUYEN=$P{S_KETCHUYEN})
		OR ($P{S_KY}='All' AND $P{S_KETCHUYEN}='All' AND (REPLACE(BILLING_CYCLE_DATE,'/','') BETWEEN $P{S_TUNGAY} AND $P{S_DENNGAY}))
		GROUP BY DRIVER_CDE, GRP, EVENT_ID, EVENT_DESC, INVOICE_NUMBER, BILLING_CYCLE_DATE,KY
		UNION ALL
		SELECT 2 SEQNO,'ZZZ' DRIVER_CDE_ORDER,' ' MA_DRIVER,' ' TEN_DRIVER,' ' GRP,' ' MA_PHI, ' ' DIEN_GIAI_MA_PHI,' ' SO_HOA_DON,' ' NGAY_HOA_DON,
		TO_NUMBER(SUM(TOTAL_CHARGE)) SO_TIEN_USD,' ' SO_TIEN_VND,SUM(DIAVITION) SO_TIEN_LECH,KY
		FROM DVNB_INVOICE_MC_TEMP   
		WHERE ($P{S_KY}<>'All' AND KY = $P{S_KY} AND KET_CHUYEN = $P{S_KETCHUYEN})
		OR ($P{S_KY}='All' AND $P{S_KETCHUYEN}<>'All' AND (REPLACE(BILLING_CYCLE_DATE,'/','') BETWEEN $P{S_TUNGAY} AND $P{S_DENNGAY}) AND KET_CHUYEN=$P{S_KETCHUYEN})
		OR ($P{S_KY}='All' AND $P{S_KETCHUYEN}='All' AND (REPLACE(BILLING_CYCLE_DATE,'/','') BETWEEN $P{S_TUNGAY} AND $P{S_DENNGAY}))
		GROUP BY KY
		) M
		LEFT JOIN DVNB_TYGIA N ON M.KY=N.KY AND N.CRD_BRN='MC'
		ORDER BY DRIVER_CDE_ORDER,SEQNO,GRP,NGAY_HOA_DON ASC]]>
		</queryString>
		<field name="MA_DRIVER" class="java.lang.String"/>
		<field name="TEN_DRIVER" class="java.lang.String"/>
		<field name="GRP" class="java.lang.String"/>
		<field name="MA_PHI" class="java.lang.String"/>
		<field name="DIEN_GIAI_MA_PHI" class="java.lang.String"/>
		<field name="SO_HOA_DON" class="java.lang.String"/>
		<field name="SO_TIEN_USD" class="java.lang.String"/>
		<field name="NGAY_HOA_DON" class="java.lang.String"/>
		<field name="SO_TIEN_VND" class="java.lang.String"/>
		<group name="GroupByDriver" isReprintHeaderOnEachPage="true">
			<groupExpression><![CDATA[$F{MA_DRIVER}]]></groupExpression>
		</group>
