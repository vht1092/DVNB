		<parameter name="s_fromdate" class="java.lang.String">
			<parameterDescription><![CDATA[]]></parameterDescription>
		</parameter>
		<parameter name="s_todate" class="java.lang.String"/>
		<parameter name="s_loaigd" class="java.lang.String"/>
		<queryString language="plsql">
			<![CDATA[SELECT NGAY_HACH_TOAN,NGAY_ADV,SUM(PHI_XU_LY_GD) PHI_XU_LY_GD_USD,
TY_GIA_TQT,SUM(TONG_PHIGD) TONG_VND
FROM DSQT_UPDATE_FEE_DAILY
WHERE NGAY_HACH_TOAN BETWEEN $P{s_fromdate} AND $P{s_todate}
AND LOAI_TIEN_TQT='USD' AND LOAI_GD=$P{s_loaigd}
AND LOAI_THE LIKE 'M%'
AND PHI_XU_LY_GD<>0
GROUP BY NGAY_HACH_TOAN,NGAY_ADV,TY_GIA_TQT
ORDER BY NGAY_HACH_TOAN ASC]]>
		</queryString>
		<field name="TONG_VND" class="java.math.BigDecimal"/>
		<field name="TY_GIA_TQT" class="java.math.BigDecimal"/>
		<field name="PHI_XU_LY_GD_USD" class="java.math.BigDecimal"/>
		<field name="NGAY_HACH_TOAN" class="java.lang.String"/>
		<field name="NGAY_ADV" class="java.lang.String"/>
		<variable name="SUM_TONG_VND" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{TONG_VND}]]></variableExpression>
			<initialValueExpression><![CDATA[new BigDecimal(0)]]></initialValueExpression>
		</variable>
